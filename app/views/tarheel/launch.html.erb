<html>
  <head>
    <%= javascript_include_tag 'aac_shim' %>
    <%= stylesheet_link_tag    'tarheel', media: 'all' %>
  </head>
  <body>
    <div id='loading'>Loading Reader...</div>
    <a id='back' href='#'>
      <span class='link'>
        <img src="data:image/png;base64," />
        <br/>Back
      </span>
    </a>
    <a id='settings' href='#'>
      <span class='link'>
        <img src='data:image/png;base64,' />
      </span>
    </a>
    <a id='next' href='#'>
      <span class='link'>
        <img src="data:image/png;base64," />
        <br/>Next
      </span>
    </a>
    <div id='title'></div>
    <a id='speak' href='#'>
      SPEAK
    </a>
    <div id='author'></div>
    <div id='cover'></div>
    <div id='image'></div>
    <div id='image2'></div>
    <div id='text'></div>
    <div id='settings_box' style='display: none;'>
      <a href='#' class='option up' id='scroll_up'>&uarr;</a>
      <a href='#' class='option down' id='scroll_down'>&darr;</a>
      <div class='settings_box_scroll'><div class='settings_box_inner'>
      <h2>Book Reader Settings
      </h2>
      <table>
        <tr>
          <td class='label'>Button Size</td>
          <td>
            <a class='option' href='#' data-key='links' data-value='small'>Small</a>
            <a class='option' href='#' data-key='links' data-value='normal'>Normal</a>
            <a class='option' href='#' data-key='links' data-value='huge'>Huge</a>
          </td>
        </tr>
        <tr>
          <td class='label'>Text Position</td>
          <td>
            <a class='option' href='#' data-key='position' data-value='text_above'>Above Pictures</a>
            <a class='option' href='#' data-key='position' data-value='text_below'>Below Pictures</a>
          </td>
        </tr>
        <tr>
          <td class='label'>Background</td>
          <td>
            <a class='option' href='#' data-key='background' data-value='white'>White</a>
            <a class='option' href='#' data-key='background' data-value='black'>Black</a>
          </td>
        </tr>
        <tr>
          <td class='label'>Reading</td>
          <td>
            <a class='option' href='#' data-key='speech' data-value='true'>Auto-Speak Text</a>
            <a class='option' href='#' data-key='speech' data-value='false'>"Speak" Button</a>
          <td/>
        </tr>
        <tr>
          <td class='label'>Add to Sentence</td>
          <td>
            <a class='option' href='#' data-key='utterance' data-value='true'>Add Text to Sentence</a>
            <a class='option' href='#' data-key='utterance' data-value='false'>Don't Add to Sentence</a>
          </td>
        </tr>
        <tr>
          <td class='label'>Highlight Words</td>
          <td>
            <a class='option' href='#' data-key='highlight' data-value='true'>Highlight Target Words</a>
            <a class='option' href='#' data-key='highlight' data-value='false'>Don't Highlight Target Words</a>
          </td>
        </tr>
      </table>
      <div style='font-size: 25px; margin-top: 20px;'>
        <a href='#' class='option' id='close_settings'>Close</a>
      </div>
      </div></div>
    </div>
    <script>
      (function() {
        var authenticity_token = "<%= form_authenticity_token %>";
        var url = location.href;
        var dom = {};
        var sound_uri = "data:audio/mp3;base64,";
        var sound = new Audio();
        sound.src = sound_uri;
        // load DOM elements for easy access
        ['loading', 'settings', 'settings_box', 'back', 'next', 'speak', 'title', 'author', 'cover', 'image', 'image2', 'text'].forEach(function(key) {
          dom[key] = document.getElementById(key);
        });
        var book = null;
        
        // helper functions
        
        // error message if there was any trouble loading
        var err = function(str) { dom.loading.innerText = str; };
        // set the image url for one of the img tags
        var pic = function(id, url) {
          if(url) {
            var img = document.createElement('img');
            img.src = url;
            dom[id].appendChild(img);
            
          }
          if(id == 'image2') {
            if(url) {
              dom.image.classList.add('double');
            } else {
              dom.image.classList.remove('double');
            }
          }
        }
        var measure_text = function(str, size) {
          // re-use canvas object for better performance
          var canvas = measure_text.canvas || (measure_text.canvas = document.createElement("canvas"));
          var context = canvas.getContext("2d");
          context.font = size + "px Arial";
          var metrics = context.measureText(str);
          return metrics;
        }
        // set the text for one of the div tags
        var text = function(id, str, highlights) {
          dom[id].innerText = str;
          if(id == 'text') {
            if(str == 'All finished!') {
              dom.text.style.font = '10vh Arial';
              return;
            }
            var too_big = true;
            var parts = [str];
            var bounds = dom.text.getBoundingClientRect();
            var available_width = bounds.width;
            var available_height = bounds.height;
            var size = available_height;
            while(too_big && size > 0 && str != "All finished!") {
              var measures = parts.map(function(p) { return measure_text(p, size); });
              var width = Math.max.apply(null, measures.map(function(m) { return m.width; }));
              var height = parts.length * (size * 1.18);
//               console.log(Math.round(width) + "x" + Math.round(height), 'within', Math.round(available_width)+"x"+Math.round(available_height));
              if(width < available_width && height < available_height) {
                too_big = false;
              } else {
                size = size - 2;
//                 console.log(size, parts[0], parts);
                if(width >= available_width && size < (available_height / 3) && parts[0].match(/\s/) && parts.length < 3) {
                  size = 100;
                  var split = parts.length + 1;
//                   console.log("splitting into " + split + " parts");
                  var new_parts = [];
                  var memo = "";
                  for(var idx = 0; idx < str.length; idx++) {
                    if(str[idx] == ' ' && memo.length > (str.length / split)) {
                      new_parts.push(memo);
                      memo = '';
                    } else {
                      memo = memo + str[idx];
                    }
                  }
                  new_parts.push(memo);
                  parts = new_parts;
                }
              }
            }
            if(highlights && highlights.length > 0 && book.settings.highlight) {
              highlight(dom[id], highlights);
            }
            dom.text.style.font = size + "px Arial"; 
          }
        }
        // clear any existing content from the page
        var clear = function() {
          for(var key in dom) {
            if(key != 'back' && key != 'next' && key != 'speak' && key != 'settings' && key != 'settings_box') {
              dom[key].innerHTML = "";
            }
          }
          dom.back.style.display = 'block';
          dom.next.style.display = 'block';
          if(book && book.settings && book.settings.links == 'small') {
            dom.back.classList.add('small');
            dom.next.classList.add('small');
            dom.back.classList.remove('huge');
            dom.next.classList.remove('huge');
          } else if(book && book.settings && book.settings.links == 'huge') {
            dom.back.classList.add('huge');
            dom.next.classList.add('huge');
            dom.back.classList.remove('small');
            dom.next.classList.remove('small');
          } else {
            dom.back.classList.remove('small');
            dom.next.classList.remove('small');
            dom.back.classList.remove('huge');
            dom.next.classList.remove('huge');
          }
          if(book && book.settings && book.settings.position == 'text_above') {
            document.body.classList.add('text_above');
          } else {
            document.body.classList.remove('text_above');
          }
          if(book && book.settings && book.settings.background == 'black') {
            document.body.classList.add('black');
          } else {
            document.body.classList.remove('black');
          }
        }
        
        function highlight(elem, keywords) {
          const flags = 'gi';
          const cls = 'highlighted_word';
          // Sort longer matches first to avoid
          // highlighting keywords within keywords.
          keywords.sort(function(a, b) { return b.length - a.length; });;
          Array.from(elem.childNodes).forEach(function(child) {
            const keywordRegex = RegExp(keywords.join('|'), flags);
            if (child.nodeType !== 3) { // not a text node
              // highlight(child, keywords, caseSensitive, cls);
            } else if (keywordRegex.test(child.textContent)) {
              const frag = document.createDocumentFragment();
              let lastIdx = 0;
              child.textContent.replace(keywordRegex, (match, idx) => {
                const part = document.createTextNode(child.textContent.slice(lastIdx, idx));
                const highlighted = document.createElement('span');
                highlighted.textContent = match;
                highlighted.classList.add(cls);
                frag.appendChild(part);
                frag.appendChild(highlighted);
                lastIdx = idx + match.length;
              });
              const end = document.createTextNode(child.textContent.slice(lastIdx));
              frag.appendChild(end);
              child.parentNode.replaceChild(frag, child);
            }
          });
        }
        
        // re-initialize the shim targets
        var update = function() {
          // it's important to wait for the callback to prevent race conditions (apparently)
          aac_shim.clear_targets(function() {
            var rect = dom.back.getBoundingClientRect();
            if(book.page > 0) {
              aac_shim.add_target({
                id: 'back',
                left: rect.left,
                top: rect.top,
                width: rect.width,
                height: rect.height,
                prompt: 'Back'
              }, function(res) {
                if(res.type == 'select') {
                  advance('back');
                }
              });
            }
            rect = dom.next.getBoundingClientRect();
            if(book.page < book.pages.length) {
              aac_shim.add_target({
                id: 'next',
                left: rect.left,
                top: rect.top,
                width: rect.width,
                height: rect.height,
                prompt: 'Next'
              }, function(res) {
                if(res.type == 'select') {
                  advance('next');
                }
              });
            }
            if(dom.speak.style.display != 'none') {
              rect = dom.speak.getBoundingClientRect();
              aac_shim.add_target({
                id: 'speak',
                left: rect.left,
                width: rect.width,
                top: rect.top,
                height: rect.height,
                prompt: 'Speak'
              }, function(res) {
                if(res.type == 'select') {
                  speak(true);
                }
              });
            }
          });
        };
        
        var book_settings = function(opt) {
          if(opt == false) {
            dom.settings_box.style.display = 'none';
          } else if(opt == 'save') {
          } else {
            dom.settings_box.style.display = 'block';
          }
        };
        // re-initialize shim targets when the window gets resized
        window.addEventListener('resize', function() {
          advance('none');
        });

        var tarheel_prefix = "https://tarheelreader.org";
        // move forward or backward within the current book, triggered by user events
        // if auto-speech is set, trigger a speak event as well
        var advance = function(direction) {
          book.page = book.page || 0;
          var prior_page = book.page;
          if(direction == 'back') {
            book.page = Math.max(0, book.page - 1);
          } else if(direction == 'next') {
            book.page = Math.min(book.pages.length, book.page + 1);
          }
          clear();
          if(book.page <= 0) {
            pic('cover', book.pages[0].image_url || book.pages[1].image_url || (tarheel_prefix + book.pages[1].url));
            text('title', book.pages[book.page].text);
            text('author', "by " + book.author);
          } else if(book.page >= book.pages.length) {
            text('text', 'All finished!');
            var a = document.createElement('a');
            a.href = book.attribution_url || ('https://tarheelreader.org/photo-credits/?id=' + book.ID);
            a.innerText = 'attribution: ' + a.href;
            a.target = '_blank';
            dom.text.appendChild(a);
          } else if(direction == 'back' || direction == 'next' || direction == 'none') {
            pic('image', book.pages[book.page].image_url || (tarheel_prefix + book.pages[book.page].url));
            pic('image2', book.pages[book.page].image2_url);
            text('text', book.pages[book.page].text, book.pages[book.page].related_words);
            if(book.settings && book.settings.speech && direction != 'none') {
              speak();
            }
          }
          if(book.page != prior_page && (direction == 'back' || direction == 'next')) {
            if(!book.settings || !book.settings.speech) {
              sound.pause();
              sound.currentTime = 0;
              sound.play();
            }
          }
          dom.settings_box.style.display = 'none';
          if(book.page <= 0 || book.page >= book.pages.length) {
            dom.settings.style.display = 'inline';
          } else {
            dom.settings.style.display = 'none';
          }
          if(book.page <= 0) {
            dom.back.style.display = 'none';
          } else {
            dom.back.style.display = 'inline';
          }
          if(book.page >= book.pages.length) {
            dom.next.style.display = 'none';
          } else {
            dom.next.style.display = 'inline';
          }
          if(book.page > 0 && book.page < book.pages.length && (!book.settings || (!book.settings.speech && !book.settings.demo))) {
            dom.speak.style.display = 'block';
          } else {
            dom.speak.style.display = 'none';
          }
          update();
        };
        
        // send the text (and image) of the current page up to the AAC container
        var speak = function(manual) {
          var page = book.pages[book.page];
          if(page && page.text) {
            if(book.settings && book.settings.utterance) {
              aac_shim.add_text(page.text, page.full_url || (page.url && (tarheel_prefix + page.url)) || null);
            } else {
              if(manual) {
                aac_shim.speak_text(page.text, 'primary');
              } else {
                aac_shim.speak_text(page.text, 'secondary');
              }
            }
          }
        };

        // start up the shim, wait for a ready response from the AAC container
        aac_shim.status(function(res) {
          if(res.error) {
            if(res.error == "No AAC Container Found") {
              var hash = location.href.split(/#/)[1];
              if(hash) {
                res = {
                  status: 'ready',
                  code: hash,
                  demo: true
                }
              } else {
                return err(res.error);
              }
            } else {
              return err(res.error);
            }
          }
          console.log("status", res);
          if(!res.status || res.status != 'ready') {
            return err("App Not Ready");
          }
          if(!res.code) {
            return err("No Code Returned");
          }
          var user_state_code = null;
          if(res.user_token) {
            user_state_code = 'Tarheel::' + (res.system_token || 'GenericAAC') + "::" + res.user_token;
          }

          var settings = null;
          try {
            // try to parse launch code as a parameter set
            settings = JSON.parse(atob(decodeURIComponent(res.code)));
            if(settings.user_token) {
              settings.user_state_code = 'Tarheel::' + (settings.system_token || 'GenericAAC') + "::" + settings.user_token;
            }
          } catch(e) {
            // otherwise fall back to launch code just being a book's id
            settings = {
              id: res.code,
              demo: res.demo
            };
          }
          if(user_state_code) {
            settings.user_state_code = user_state_code;
          }

          var xhr = new XMLHttpRequest();
          xhr.onreadystatechange = function() {
            if(this.readyState == 4 && this.status == 200) {
              res = JSON.parse(this.responseText);
              console.log(res);
              book = res;
              book.settings = settings;
              if(res.user_state) {
                for(var key in res.user_state) {
                  book.settings[key] = res.user_state[key];
                }
              }
              book.settings.links = book.settings.links || 'normal';
              book.settings.position = book.settings.position || 'text_below';
              book.settings.background = book.settings.background || 'white';
              book.settings.highlight = book.settings.highlight || 'true';
              // these event listeners are here for fallback demo purposes, but
              // are not actually used when launched via AAC
              dom.back.addEventListener('click', function(event) {
                event.preventDefault();
                advance('back');
              });
              dom.next.addEventListener('click', function(event) {
                event.preventDefault();
                advance('next');
              });
              dom.speak.addEventListener('click', function(event) {
                event.preventDefault();
                speak(true);
              });
              dom.settings.addEventListener('click', function(event) {
                event.preventDefault();
                book_settings();
              });
              dom.settings_box.addEventListener('click', function(event) {
                event.preventDefault();
                settings_event(event.target);
              });
              advance('back');
            } else if(this.readyState == 4) {
              if(!navigator.onLine) {
                err("Books Can't Load When Offline");
              } else {
                err("Failed to Load Book, please check settings");
              }
            }
          };
          xhr.open('GET', "/tarheel/book?id=" + encodeURIComponent(settings.id) + "&user_state_code=" + (settings.user_state_code || ''), true);
          xhr.send();
          
          // Settings handling, this is more advanced code than you'll need for a basic shim
          aac_shim.listen(function(event) {
            if(event.x_percent !== undefined) {
              var x = Math.round(event.x_percent * window.innerWidth);
              var y = Math.round(event.y_percent * window.innerHeight);
              var elem = document.elementFromPoint(x, y);
              var original_element = elem;
              while(elem && !elem.id && elem.tagName != 'BODY') {
                elem = elem.parentNode;
              }
              if(event.aac_type == 'select' || event.type == 'gazedwell' || event.type == 'mouseup' || event.type == 'click' || event.type == 'touchend' || event.type == 'scanselect') {
                if(elem && elem.id == 'settings') {
                  book_settings();
                } else if(elem && elem.id == 'close_settings') {
                  book_settings(false);
                } else if(elem && elem.id == 'save_settings') {
                  book_settings('save');
                } else if(elem && elem.id == 'scroll_up') {
                  book_settings('up');
                } else if(elem && elem.id == 'scroll_down') {
                  book_settings('down');
                } else if(elem && elem.id == 'settings_box') {  
                  settings_event(original_element);
                } else if(elem && elem.classList.contains('highlighted_word')) {
                  trigger_word(original_element);
                }
              }
            }
          });
          
          document.body.addEventListener('click', function(event) {
            if(event.target.classList.contains('highlighted_word')) {
              trigger_word(event.target);
            }
          });
          
          var trigger_word = function(elem) {
            aac_shim.speak_text(elem.innerText, 'primary');
            Array.from(document.getElementsByClassName('highlighted_word')).forEach(function(elem) {
              elem.classList.remove('speaking');
            });
            elem.classList.add('speaking');
            setTimeout(function() {
              elem.classList.remove('speaking');
            }, 3000);
          };
          
          var settings_event = function(elem) {
            console.log("hit elem", elem);
            if(elem.id == 'close_settings') {
              book_settings(false);
            } else if(elem.id == 'save_settings') {
              book_settings('save');
            } else if(elem.id == 'scroll_up') {
              book_settings('up');
            } else if(elem.id == 'scroll_down') {
              book_settings('down');
            } else {
              if(elem.getAttribute('data-key') && elem.getAttribute('data-value')) {
                var val = elem.getAttribute('data-value');
                if(val === 'true') { val = true; }
                else if(val === 'false') { val = false; }
                book.settings[elem.getAttribute('data-key')] = val;
              }
            }
            book_settings('update');
          };
        });
        
        var book_settings = function(opt) {
          if(opt == false) {
            dom.settings_box.style.display = 'none';
            if(book.settings.user_state_code) {
              var xhr = new XMLHttpRequest();
              xhr.onreadystatechange = function() {
                if(this.readyState == 4 && this.status == 200) {
                } else if(this.readyState == 4) {
                }
              };
              xhr.open('POST', "/states/" + book.settings.user_state_code);
              xhr.setRequestHeader("Content-Type", "application/json");
              book.settings.authenticity_token = authenticity_token;
              xhr.send(JSON.stringify(book.settings));
            }
            advance('none');
          } else if(opt == 'update') {
          } else {
            dom.settings_box.style.display = 'block';
          }
          var scroll = document.getElementsByClassName('settings_box_scroll')[0];
          var inner_rect = document.getElementsByClassName('settings_box_inner')[0].getBoundingClientRect();
          var outer_rect = scroll.getBoundingClientRect();
          console.log(outer_rect.height, inner_rect.height);
          if(outer_rect.height < inner_rect.height) {
            document.getElementById('scroll_up').style.display = 'block';
            document.getElementById('scroll_down').style.display = 'block';
          } else {
            document.getElementById('scroll_up').style.display = 'none';
            document.getElementById('scroll_down').style.display = 'none';
          }
          if(opt == 'up') {
            scroll.scrollTop = (scroll.scrollTop || 0) - 70;
          } else if(opt == 'down') {
            scroll.scrollTop = (scroll.scrollTop || 0) + 70;
          }

          var list = document.getElementsByClassName('option');
          for(var idx = 0; idx < list.length; idx++) {
            var elem = list[idx];
            var key = elem.getAttribute('data-key');
            if(elem.id != 'save_settings') {
              elem.classList.remove('active');
            }
            if(key && ((book.settings[key] || false).toString() == elem.getAttribute('data-value'))) {
              elem.classList.add('active');
            }
          }
        };

      })();
    </script>
  </body>
</html>